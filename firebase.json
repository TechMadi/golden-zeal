{
  "hosting": {
    "public": "dist/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
     {
        "source": "/crm/**",
        "destination": "/admin-golden-zeal/browser/index.html"
      },
      {
        "source": "/**",
        "destination": "/golden-zeal/browser/index.html"
      }
    ]
  }
}
